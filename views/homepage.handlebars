<!-- Loop over each object in the projects array -->


{{#each blogs }}

<div class="row mb-4 blog box">
  <div class="col-md-5">
    <h2>
      <!-- Call helper function with triple brackets to render the emoji span as HTML -->
      <a href="/">{{title}}
      </a>
    </h2>
    <p>Created by {{user.name}} on {{format_date date_created}}</p>
  </div>
  <div class="col-md-7">
    <p>
      {{content}}
    </p>
  </div>
</div>
{{/each}}





